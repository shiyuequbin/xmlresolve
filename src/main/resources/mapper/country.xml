<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot_xml.xml_pfa.mapper.CountryMapper">
    <resultMap id="BaseResultMap" type="com.springboot_xml.xml_pfa.domain.Country" >
        <id column="ID" property="ID" jdbcType="INTEGER" />
        <result column="COUNTRYDETAILS_ID" property="COUNTRYDETAILS_ID" jdbcType="INTEGER" />
        <result column="COUNTRY_COUNTRYTYPE" property="COUNTRY_COUNTRYTYPE" jdbcType="VARCHAR" />
        <result column="INSERT_DATE"  property="insertDate"  jdbcType="DATE"/>
       <!-- <result column="PERSON_PROFILENOTES" property="PERSON_PROFILENOTES" javaType="java.sql.Blob" jdbcType="BLOB" typeHandler="org.apache.ibatis.type.BlobTypeHandler" />
        <result column="INERT_DATE"  property="insertDate"  jdbcType="DATE"/>-->

    </resultMap>

    <sql id="Base_Column_List" >
       ID,COUNTRYDETAILS_ID,COUNTRY_COUNTRYTYPE,INSERT_DATE
    </sql>
    <insert id="insert" parameterType="com.springboot_xml.xml_pfa.domain.Country">
        insert into TBL_COUNTRY (
        ID,COUNTRYDETAILS_ID,COUNTRY_COUNTRYTYPE,INSERT_DATE
        )
        values
        (#{ID},#{COUNTRYDETAILS_ID},#{COUNTRY_COUNTRYTYPE},#{insertDate})
       <!-- <foreach collection="list" item="Person" separator=",">
            (#{personId},#{Person.PFA_ID},#{Person.PERSON_ID},#{Person.PERSON_ACTION},#{Person.PERSON_DATE},
            #{Person.PERSON_GENDER},#{Person.PERSON_ACTIVESTATUS},
            #{Person.id},#{Person.PERSON_PROFILENOTES},#{Person.insertDate})
        </foreach>-->
    </insert>
</mapper>