<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot_xml.xml_pfa.mapper.Description3NameMapper">
    <resultMap id="BaseResultMap" type="com.springboot_xml.xml_pfa.domain.Description3Name" >
