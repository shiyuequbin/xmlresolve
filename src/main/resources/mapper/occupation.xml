<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot_xml.xml_pfa.mapper.OccupationMapper">
    <resultMap id="BaseResultMap" type="com.springboot_xml.xml_pfa.domain.Occupation" >
        <id column="ID" property="ID" jdbcType="INTEGER" />
        <result column="PFA_ID" property="PFA_ID" jdbcType="INTEGER" />
        <result column="OCCUPATION_CODE" property="OCCUPATION_CODE" jdbcType="VARCHAR" />
        <result column="OCCUPATION_NAME" property="OCCUPATION_NAME" jdbcType="VARCHAR" />
        <result column="INSERT_DATE"  property="insertDate"  jdbcType="DATE"/>
       <!-- <result column="PERSON_PROFILENOTES" property="PERSON_PROFILENOTES" javaType="java.sql.Blob" jdbcType="BLOB" typeHandler="org.apache.ibatis.type.BlobTypeHandler" />
        <result column="INERT_DATE"  property="insertDate"  jdbcType="DATE"/>-->

    </resultMap>

    <sql id="Base_Column_List" >
       ID,PFA_ID,OCCUPATION_CODE,OCCUPATION_NAME,INSERT_DATE
    </sql>
    <insert id="insert" parameterType="com.springboot_xml.xml_pfa.domain.Occupation">
        insert into TBL_OCCUPATION (
        ID,PFA_ID,OCCUPATION_CODE,OCCUPATION_NAME,INSERT_DATE
        )
        values
        (#{ID},#{PFA_ID},#{OCCUPATION_CODE},#{OCCUPATION_NAME},#{insertDate})
       <!-- <foreach collection="list" item="Person" separator=",">
            (#{personId},#{Person.PFA_ID},#{Person.PERSON_ID},#{Person.PERSON_ACTION},#{Person.PERSON_DATE},
            #{Person.PERSON_GENDER},#{Person.PERSON_ACTIVESTATUS},
            #{Person.id},#{Person.PERSON_PROFILENOTES},#{Person.insertDate})
        </foreach>-->
    </insert>
</mapper>